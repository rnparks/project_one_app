<!-- Google Balance Sheet -->


<div CLASS="financial_table">
<TABLE CLASS="financial_table">
   <TR>
      <TH COLSPAN="4">
      <H3>Google Balance Sheet</H3>
      </TH>
   </TR>
   <TR ALIGN="right">
      <TH> </TH>
      <TH>2012</TH>
      <TH>2013</TH>
   </TR>

   <% FinancialParse::GOOGLE_BALANCE_SHEET.each_key do |key| %>
   <TR>
      <TD ALIGN="LEFT" class="key">  <%= key %>  </TD>
      <TD ALIGN="RIGHT">  <%= FinancialParse::GOOGLE_BALANCE_SHEET[key][:"2012"] %> </TD>
      <TD ALIGN="RIGHT">  <%= FinancialParse::GOOGLE_BALANCE_SHEET[key][:"2013"] %> </TD>
   </TR>
   <% end %>

</TABLE>
</div>
